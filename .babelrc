{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "styled-components",
    "@babel/transform-react-jsx",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-transform-async-to-generator"
  ]
}
